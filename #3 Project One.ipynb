   "source": [
    "#This block creates a table of industry names that are in sector 2\n",
    "#Sorted by rbr_id in ascending order\n",
    "#NOTE: SHOULD PROBABLY MAKE THIS ALPHABETICAL (FIRST LETTER AFTER underscore)\n",
    "\n",
    "rs = data('locus_rbrs', 'rs')\n",
    "\n",
    "result=rs.where(rs.taxonomy=='Sector_v2').select('rs.name','rs.rbr_id').limit(1000)\n",
    "\n",
    "df=pd.DataFrame(result)\n",
    "df.sort_values(by=['rbr_id'], inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "#puts all rbr_ids into a list\n",
    "id_list=[]\n",
    "name_list=[]\n",
    "menu_list=[]\n",
    "\n",
    "for index,row in df.iterrows():\n",
    "    id_list.append(row['rbr_id'])\n",
    "    name_list.append(row['name'])\n",
    "menu_list=list(zip(name_list,id_list))\n",
    "#print(menu_list)"
   ]
