   "source": [
    "#finding children with different naics codes than the HQ id\n",
    "\n",
    "#Strategy:\n",
    "#joining businesses with business on hq_id of the first = business_id of the second\n",
    "#the naics codes from the first table are the children's naics codes\n",
    "#the naics codes from the second table are the hq_naics codes\n",
    "#find where they are different\n",
    "\n",
    "b = data('database_usa_business20200301s', 'b')\n",
    "hq = data('database_usa_business20200301s', 'hq')\n",
    "\n",
    "joined = (b.joins(hq, on=(b.hq_id == hq.dbusa_business_id)))\n",
    "result = joined.where(b.naics01_code!=hq.naics01_code).limit(100)\n",
    "\n",
    "df=pd.DataFrame(result)\n",
    "df.head()\n"
   ]
